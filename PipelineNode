#!/usr/bin/env groovy
node {
    checkout scm    
    stage ('Build') {
        withMaven ( maven: 'Maven Test) {
                   sh 'mvn compile'
                   }
    }   
    stage ('Test') {
        sh 'mvn test'
        junit '**/target/*.xml'
    }
    stage ('Deploy') {
        sh 'mvn package'
    }
}
